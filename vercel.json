{
  "framework": "vite",
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "functions": {
    "api/stripe/*.ts": {
      "runtime": "@vercel/node@latest"
    }
  },
  "routes": [
    {
      "src": "/api/stripe/products",
      "dest": "/api/stripe/products.ts"
    },
    {
      "src": "/api/stripe/create-checkout-session",
      "dest": "/api/stripe/create-checkout-session.ts"
    },
    {
      "src": "/webhook",
      "dest": "/api/webhook.ts"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ]
}
